#:kivy 1.8.0

<Main_Footer>:
    orientation:"horizontal"

    canvas:
        Color:
            rgb: 0.128,0.128,0.128
        Rectangle:
            size: self.size
            pos: self.pos


<Main_header>:
    orientation:"horizontal"

    canvas:
        Color:
            rgb: 0.128,0.128,0.128
        Rectangle:
            size: self.size
            pos: self.pos
            

    Label:
        text: "Acces Programme Principal"


    Button:
        id: Quit
        size_hint_x: 0.1
        text: "X"
        on_release: app.stop()


<Main_body>:
    orientation: "vertical"
  
    Main_header:
        id:header
        size_hint_y: 0.1

    Label:
        text: "lol"
        
    
    Main_Footer:
        id: main_footer
        size_hint_y: 0.05


